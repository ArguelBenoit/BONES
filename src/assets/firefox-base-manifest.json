{
  "manifest_version": 2,
  "name": "",
  "version": "",
  "description": "",
  "homepage_url": "",
  "author": "",
  "license": "",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-96.png",
    "default_popup": "popup.html",
    "default_title": "Bones"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}